{
  "name": "transparent-win",
  "displayName": "transparent-win",
  "description": "make the window transparent",
  "version": "0.0.1",
  "author": "ID-wj",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ID-wj/transparent-win.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Transparent Win",
      "properties": {
        "transparent-win.transparency": {
          "type": "number",
          "default": 0.9,
          "description": "the transparent num must be between 0.2 and 1",
          "minimum": 0.2,
          "maximum": 1
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ffi-napi": "^4.0.3",
    "ref-napi": "^3.0.3"
  }
}
